{
    "manifest_version": 2,
    "name": "W for Weather",
    "version": "1.0.0",
    "description": "A simple minimalist weather App",
    "icons": {"16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png" },
    "browser_action": {
       "default_icon": "icon_48.png",
       "default_popup": "popup.html"
    },
    "permissions":["activeTab"]
}